                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "grant_type": {
                                        "type": "string",
                                        "title": "Grant Type",
                                        "description": "Grant type"
                                    },
                                    "username": {
                                        "type": "string",
