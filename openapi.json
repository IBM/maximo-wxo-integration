                                    },
                                    "client_id": {
                                        "type": "string",
                                        "title": "Client Id",
                                        "description": "The client id of the user"

                                    },
                                    "client_secret": {
                                        "type": "string",                                        
                                        "title": "Client Secret",
                                        "description": "The client id of the user"
                                    }
                                },
                                "type": "object",
                                "required": [
                                    "username",
                                    "password"
                                ],
                                "title": "Body_login_token_post"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "error": {
                                            "type": "string",
                                            "description": "The error message"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "error": {
